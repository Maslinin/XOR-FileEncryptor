{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CMake Project",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/FileEncryptor.exe",
            "args": ["-e", "-g", "-i", "${workspaceFolder}/build/bin/enc.exe", "-o", "${workspaceFolder}/build/bin/dec.exe"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "preLaunchTask": "CMake: build",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}